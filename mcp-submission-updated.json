  },
  "license": "MIT",
  "categories": ["business", "finance", "accounting"],
  "tags": ["bookkeeper", "cpa", "accountant", "tax", "payroll", "quickbooks", "x402"],
  "runtime": {
    "type": "remote",
    "protocol": "http",
    "url": "https://autarchic-unbodied-bryson.ngrok-free.dev",
    "paths": {
      "mcp": "/",
      "health": "/health"
    }
  },
  "payment": {
    "type": "x402",
    "asset": "USDC",
    "network": "base",
    "price": "0.10",
    "receiver": "0x708AF34B155834B1e45B4B4b5933486a4e173C4e"
  },
  "capabilities": {
    "tools": [
      {
        "name": "find_bookkeepers",
        "description": "Search for bookkeepers by location and service",
        "parameters": {
          "service": {
            "type": "string",
            "description": "Type of service needed (bookkeeping, tax, payroll, etc.)",
            "required": true
          },
          "location": {
            "type": "string",
            "description": "City and state (e.g., 'Miami, FL')",
            "required": true
          },
          "min_rating": {
            "type": "number",
            "description": "Minimum rating (1-5)",
            "default": 4.0
          }
        },
        "returns": {
          "type": "array",
          "description": "List of verified bookkeepers with contact info and ratings"
        }
      }
    ]
  },
  "examples": [
    {
      "description": "Find bookkeepers in Miami",
      "request": {
        "service": "bookkeeping",
        "location": "Miami, FL"
      }
    },
    {
      "description": "Find CPAs for tax preparation in Tampa",
      "request": {
        "service": "tax preparation",
        "location": "Tampa, FL",
        "min_rating": 4.5
      }
    }
  ],
  "data_coverage": {
    "total_professionals": 950,
